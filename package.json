{
  "name": "platformio-ide-terminal",
  "main": "./lib/platformio-ide-terminal",
  "version": "2.0.0",
  "description": "A terminal package for Atom, complete with themes, API and more for PlatformIO IDE. Fork of terminal-plus.",
  "author": "Jeremy Ebneyamin",
  "keywords": [
    "PlatformIO",
    "terminal-plus",
    "terminal",
    "plus",
    "panel",
    "tty"
  ],
  "homepage": "https://atom.io/packages/platformio-ide-terminal2",
  "repository": "https://github.com/platformio/platformio-atom-ide-terminal2",
  "license": "MIT",
  "engines": {
    "atom": ">=1.2.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.0",
    "pty.js": "https://github.com/jeremyramin/pty.js/tarball/28f2667",
    "term.js": "https://github.com/jeremyramin/term.js/tarball/master",
    "underscore": "^1.8.3"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "runInTerminal": {
      "description": "Allow to run commands in terminal.",
      "versions": {
        "0.14.5": "provideRunInTerminal"
      }
    }
  }
}
